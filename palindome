#include <iostream>
using namespace std;

int main(){
  char string[20];
  int i,length;
  int flag=0;

  cin>>string;
  length= strlen(string);

  for (i=0;i<length;i++){
     if(string[i] != string[length-i-1])
    {
            flag = 1;
            break;
    }
  }
  if (flag){
      cout<< "no \n"; }
  else {
       cout << " yes \n";
   }
}
